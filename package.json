{
  "name": "mongo-connector-poc",
  "version": "0.1.0",
  "type": "commonjs",
  "scripts": {
    "start": "node ./dist/main.js",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix",
    "build": "tsc",
    "build:watch": "tsc -w",
    "test": "jest",
    "test:cov": "jest --coverage"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testMatch": [ "**/tests/**/*.test.ts" ],
    "transform": { "^.+\\.ts$": "ts-jest" },
    "moduleFileExtensions": [ "ts", "js", "json", "node" ]
  },
  "devDependencies": {
    "@eslint/js": "^9.20.0",
    "@jest/globals": "^29.7.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.4",
    "eslint": "^9.20.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.6",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.24.1"
  },
  "dependencies": {
    "mongoose": "^8.10.1"
  }
}
